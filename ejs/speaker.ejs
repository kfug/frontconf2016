  <% speakers.forEach(function(speaker){ %>
  <section class="column speaker" id="<%=speaker.code%>">
    <div class="inner">
    <!--<div class="label time"></div>-->
    <!--<div class="label hole a-hole"></div>-->
      <div class="cf">
      <div class="speaker-img">
          <p class="img">
            <img src="./images/speakers/<%=speaker.code%>.jpg">
          </p><!-- /.img -->
        </div><!-- /.speaker-img -->
        <div class="speaker-detail">
          <span class="speakerlabel time-label"><%=speaker.time %></span>
          <span class="speakerlabel hall-label"><%=speaker.hall %>ホール</span>
          <!-- <span class="speakerlabel category-label"><%=speaker.category %></span> -->
          <h2><%=speaker.title%></h2>
          <h1><span class="speaker-name"><%=speaker.name %></span></h1>
          <div class="linklist">
             <a href="<%=speaker.link.twitter %>" target="_blank" class="link"><i class="icon-twitter"></i></a> <a href="<%=speaker.link.facebook %>" target="_blank" class="link"><i class="fa fa-facebook-official"></i></a> <a href="<%=speaker.link.blog %>" target="_blank" class="link"><i class="fa fa-desktop"></i></a>
          </div><!-- /.linklist -->
          <p class="description-txt"><%=speaker.description %></p>
        </div><!-- /.speaker-detail -->
      </div><!-- /.cf -->
      <ul class="tag">
        <li><span><%=speaker.tag %></span></li>
      </ul>
    </div><!-- /.inner -->
  </section><!-- /.column speaker -->
  <% });%>
  <section>
    <p class="nobtntxt">and more ...</p>
