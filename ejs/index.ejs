	<section id="staff" class="cont">
		<div class="inner">
			<h2><i class="fa fa-users"></i><span>Staff</span></h2>

			<!--<p class="cs">coming soon!</p><!-- /.cs -->
			<ul class="staff-list">
				<% staffs.forEach(function(staff){ %>
				<li>
				<%if(staff.link){%>
                    <a href="<%=staff.link%>" target="_blank"><img src="./images/staffs/<%=staff.image%>">
						<span><%=staff.name%></span>
					</a>
				<%}else{%>
                    <img src="./images/staffs/<%=staff.image%>">
					<span><%=staff.name%></span>
				<%}%>
				</li>
				<% })%>
			</ul><!-- /.sponsor-list -->

		</div><!-- /.inner -->
	</section><!-- /#staff -->


